<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="406.0" prefWidth="735.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.UserController">
   <children>
      <ImageView fitHeight="407.0" fitWidth="814.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@product-9.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tvUser" layoutX="15.0" layoutY="73.0" onMouseClicked="#Select" prefHeight="200.0" prefWidth="631.0">
        <columns>
            <TableColumn fx:id="colid" minWidth="0.0" prefWidth="0.0" text="ID" />
          <TableColumn fx:id="colnom" prefWidth="81.0" text="Nom" />
          <TableColumn fx:id="colprenom" prefWidth="83.0" text="Prenom" />
            <TableColumn fx:id="colrole" minWidth="1.0" prefWidth="66.0" text="Role" />
            <TableColumn fx:id="colemail" prefWidth="88.0" text="E-mail" />
            <TableColumn fx:id="colpseudo" prefWidth="87.0" text="Pseudo" />
            <TableColumn fx:id="colpassword" prefWidth="73.0" text="Password" />
            <TableColumn fx:id="colnumero" prefWidth="59.0" text="Numero" />
            <TableColumn fx:id="coladresse" prefWidth="91.0" text="Adresse" />
        </columns>
      </TableView>
      <Button fx:id="btnSupprimer" layoutX="509.0" layoutY="290.0" mnemonicParsing="false" onAction="#Delete" text="Supprimer">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="modiferbtn" layoutX="78.0" layoutY="290.0" mnemonicParsing="false" onAction="#Modifier" text="Modifier">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Retour" text="Retour" />
      <Label layoutX="248.0" layoutY="9.0" text="Gestion User" textFill="RED">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
