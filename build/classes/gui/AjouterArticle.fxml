<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="454.0" prefWidth="893.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AjouterArticleController">
   <children>
      <Label fx:id="labelaj" layoutX="214.0" layoutY="14.0" text="Ajouter un article">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <VBox layoutX="175.0" layoutY="99.0" prefHeight="283.0" prefWidth="241.0" spacing="12.0">
         <children>
            <TextField fx:id="tftitre" />
            <TextField fx:id="tfdescription" />
            <DatePicker fx:id="tfdate" />
            <TextField fx:id="tfimg" />
            <ComboBox fx:id="cbjeu" onMouseClicked="#remplirCB" prefWidth="150.0" />
         </children>
      </VBox>
      <Button fx:id="btnajouter" layoutX="539.0" layoutY="297.0" mnemonicParsing="false" onMouseClicked="#AjouterArticle" text="Ajouter">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnannuler" layoutX="687.0" layoutY="297.0" mnemonicParsing="false" onAction="#annuler" text="Retour">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="btntraduire" layoutX="23.0" layoutY="412.0" mnemonicParsing="false" onAction="#traduire" text="Traduire" />
      <Label fx:id="titrelabel" layoutX="45.0" layoutY="99.0" prefHeight="27.0" prefWidth="64.0" text="titre">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="desclabel" layoutX="44.0" layoutY="150.0" prefHeight="27.0" prefWidth="123.0" text="Description">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="labelimg" layoutX="43.0" layoutY="227.0" prefHeight="27.0" prefWidth="70.0" text="Image">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="datelabel" layoutX="43.0" layoutY="189.0" prefHeight="27.0" prefWidth="64.0" text="Date">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="jeulabel" layoutX="48.0" layoutY="270.0" text="Jeu">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
