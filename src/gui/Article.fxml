<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="459.0" prefWidth="999.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ArticleController">
   <children>
      <TableView fx:id="tvarticle" layoutX="93.0" layoutY="94.0" onMouseClicked="#Select" prefHeight="254.0" prefWidth="857.0">
        <columns>
          <TableColumn fx:id="colid" minWidth="0.0" prefWidth="0.0" text="ID" />
          <TableColumn fx:id="colnomjeu" prefWidth="161.0" text="Id_jeu" />
            <TableColumn fx:id="coltitre" prefWidth="161.0" text="Titre" />
            <TableColumn fx:id="coldescription" prefWidth="161.0" text="Description" />
            <TableColumn fx:id="coldate" prefWidth="149.0" text="Date" />
            <TableColumn fx:id="colimg" prefWidth="184.0" text="Image" />
        </columns>
      </TableView>
      <Button fx:id="btndelete" layoutX="677.0" layoutY="387.0" mnemonicParsing="false" onAction="#Delete" text="Supprimer">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnmodifier" layoutX="556.0" layoutY="387.0" mnemonicParsing="false" onAction="#modifier" prefHeight="39.0" prefWidth="107.0" text="Modifier">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btninsert" layoutX="113.0" layoutY="387.0" mnemonicParsing="false" onAction="#interfaceajouterArticle" prefHeight="39.0" prefWidth="107.0" text="Insert">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btwreturn" layoutX="37.0" layoutY="39.0" mnemonicParsing="false" onAction="#RetrunInterface" text="Return">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
