<AnchorPane id="AnchorPane" prefHeight="474.0" prefWidth="1044.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.JeuxController">
   <children>
      <TableView fx:id="tvJeux" layoutX="415.0" layoutY="78.0" onMouseClicked="#Select" prefHeight="319.0" prefWidth="569.0">
        <columns>
          <TableColumn fx:id="colid" prefWidth="46.0" text="ID" />
            <TableColumn fx:id="colnomjeu" prefWidth="134.0" text="Nom de jeu" />
            <TableColumn fx:id="colimagejeu" prefWidth="347.0" text="Image de jeu" />
        </columns>
      </TableView>
      <VBox layoutX="22.0" layoutY="112.0" prefHeight="141.0" prefWidth="127.0" spacing="15.0">
         <children>
            <Label text="Nom jeu">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label text="Image jeu">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="149.0" layoutY="119.0" prefHeight="153.0" prefWidth="208.0" spacing="10.0">
         <children>
            <TextField fx:id="tfnomjeu" />
            <TextField fx:id="tfimg" />
         </children>
      </VBox>
      <Button fx:id="btninsert" layoutX="37.0" layoutY="293.0" mnemonicParsing="false" onAction="#Ajouter" prefHeight="39.0" prefWidth="96.0" text="Ajouter">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Lighting bumpInput="$null" diffuseConstant="1.61" specularConstant="1.35" specularExponent="18.53" surfaceScale="7.48">
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect>
      </Button>
      <Button fx:id="btnupdate" layoutX="164.0" layoutY="293.0" mnemonicParsing="false" onAction="#update" prefHeight="39.0" prefWidth="120.0" text="mise a jour">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btndelete" layoutX="281.0" layoutY="293.0" mnemonicParsing="false" onAction="#Delete" prefHeight="39.0" prefWidth="103.0" text="Delete">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="188.0" layoutY="14.0" text="Gestion Jeux">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <Button fx:id="tfreturn" layoutX="53.0" layoutY="409.0" mnemonicParsing="false" onAction="#RetrunInt" prefHeight="31.0" prefWidth="96.0" text="Retourner" />
   </children>
